import numpy as np
from numpy import sqrt

n = int(input("введите размерность"))
a = np.array([[1,2],[2,3],[7,12]])
b = np.array([[2,1],[3,4],[6,6]])
mat=np.zeros((n,n), dtype=float)
for i in range(n):
    for j in range(n):
        mat[i][j]=sqrt((a[i][0]-b[j][0])**2+(a[i][1]-b[j][1])**2)
print(mat)
